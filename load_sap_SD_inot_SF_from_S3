
----------------create link to david's s3 storage
use role accountadmin;
use warehouse demo_wh;
use database "SNOWFLAKE_DEMO_RESOURCES";
use schema sap;

CREATE or REPLACE STORAGE INTEGRATION s3_int_dr
  TYPE = EXTERNAL_STAGE
  STORAGE_PROVIDER = S3
  ENABLED = TRUE
  STORAGE_AWS_ROLE_ARN = 'arn:aws:iam::160369543881:role/SnowflakeS3accessrole'
  STORAGE_ALLOWED_LOCATIONS = ('s3://davidandybucket/sap/');
  
DESC INTEGRATION s3_int_dr;
   
grant create stage on schema "SNOWFLAKE_DEMO_RESOURCES"."SAP" to role sysadmin;
grant usage on integration s3_int_dr to role sysadmin;

use role accountadmin;
 create or replace stage sap
  storage_integration = s3_int_dr
  url = 's3://davidandybucket/sap/'
  file_format = 'sap_pipe';
  
list @sap;

